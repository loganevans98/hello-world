<%= erb 'roulette_header'.to_sym %>

<form class="search-form" action="/search_giphy" method="post">
  <input placeholder="Enter a Topic" name="query_text" type="text" />
  <button class="search-button">Search</button>
</form>
<h2><a href= "/queries">Past Searches</a></h2>
<p>You searched for: <span class="label"> <%= @query_text %></span></p>

<img src="<%= @result.image_url %>">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
<script src="javascripts/results_form.js"></script>
